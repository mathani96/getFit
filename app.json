{
  "expo": {
    "name": "getFit",
    "slug": "getFit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",   
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#f5f5f5"
      }
    },
    
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-video"
    ]
  }
}
